{
	"name": "@vanyauhalin/eslint-config",
	"version": "0.0.11",
	"description": "vanyauhalin's ESLint config",
	"keywords": [
		"config",
		"eslint-config",
		"eslint",
		"formatter",
		"linter"
	],
	"homepage": "https://eslint-config.vanyauhalin.me/",
	"bugs": {
		"url": "https://github.com/vanyauhalin/eslint-config/issues/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vanyauhalin/eslint-config.git"
	},
	"license": "MIT",
	"author": {
		"name": "Ivan Uhalin",
		"email": "hi@vanyauhalin.me",
		"url": "https://vanyauhalin.me/"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"types": "./dist/main.d.ts"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"files": [
		"dist/"
	],
	"scripts": {
		"build-docs": "eslint-config-inspector build --config main.ts --outDir docs",
		"build-package": "tsc --project tsconfig.build.json",
		"build-package-json": "clear-package-json package.json --output package.json",
		"lint-code": "eslint --no-warn-ignored --concurrency=auto .github *.*",
		"lint-types": "tsc",
		"serve-docs": "eslint-config-inspector --config main.ts --port 8080 --open false"
	},
	"dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "4.6.0",
		"@eslint/markdown": "7.5.1",
		"@stylistic/eslint-plugin": "5.7.1",
		"@typescript-eslint/eslint-plugin": "8.54.0",
		"@typescript-eslint/parser": "8.54.0",
		"@typescript-eslint/utils": "8.54.0",
		"eslint": "9.39.2",
		"eslint-config-flat-gitignore": "2.1.0",
		"eslint-plugin-ascii": "2.0.0",
		"eslint-plugin-de-morgan": "2.0.0",
		"eslint-plugin-depend": "1.4.0",
		"eslint-plugin-es-x": "9.3.0",
		"eslint-plugin-github": "6.0.0",
		"eslint-plugin-import-newlines": "1.4.0",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-jsdoc": "62.5.0",
		"eslint-plugin-jsonc": "2.21.0",
		"eslint-plugin-math": "0.13.1",
		"eslint-plugin-n": "17.23.2",
		"eslint-plugin-no-unsanitized": "4.1.4",
		"eslint-plugin-node-dependencies": "1.3.0",
		"eslint-plugin-package-json": "0.88.2",
		"eslint-plugin-prefer-let": "4.0.1",
		"eslint-plugin-promise": "7.2.1",
		"eslint-plugin-regexp": "3.0.0",
		"eslint-plugin-security": "3.0.1",
		"eslint-plugin-toml": "1.0.3",
		"eslint-plugin-unicorn": "62.0.0",
		"eslint-plugin-wc": "3.0.2",
		"eslint-plugin-yml": "3.0.0",
		"jsonc-eslint-parser": "2.4.2",
		"toml-eslint-parser": "1.0.3",
		"typescript": "5.9.3",
		"yaml-eslint-parser": "2.0.0"
	},
	"devDependencies": {
		"@eslint/config-inspector": "1.4.2",
		"clean-publish": "6.0.1"
	},
	"engines": {
		"node": ">=22.18.0"
	}
}
